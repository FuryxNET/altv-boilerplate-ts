{
  "name": "altv-boilerplate-ts",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch ./modules -e ts -x \"tsc && node ./scripts/build.js && cd altv-server && altv-server.exe\"",
    "win": "tsc && node ./scripts/build.js -m && cd altv-server && altv-server.exe",
    "linux": "tsc && node ./scripts/build.js -m && cd altv-server && ./altv-server",
    "update": "node ./scripts/update.js"
  },
  "devDependencies": {
    "@altv/types-client": "^2.5.8",
    "@altv/types-natives": "^1.5.3",
    "@altv/types-server": "^2.7.1",
    "@altv/types-shared": "^1.4.5",
    "fs-extra": "^11.1.1",
    "nodemon": "^2.0.22",
    "terser": "^5.17.1",
    "typescript": "^5.0.4"
  }
}
